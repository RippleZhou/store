/* pages/order/returns/returns.wxss */
/***中间**/
.vir_box{
  margin-top: 88rpx;padding-bottom: 100rpx;
}
.return_cons{
  padding:20rpx;
}
.w_myOnline_list{
 margin-bottom: 20rpx;background: #fff;border-radius: 10rpx;width:100%;
}
.w_myOnline_states{
  width:100%;padding:0 20rpx;text-align:right;line-height: 70rpx;font-size:28rpx;color:#E61817;
  display:flex;display:-webkit-flex;display: -ms-flexbox;
}
.w_myOnline_states view{
  display:block;width:70%;text-align:left;font-size: 26rpx;color:#E61817;
}
.w_myOnline_states view label{color:#333;margin-right: 20rpx;display:inline-block;font-size: 26rpx;}
.w_myOnline_states text{
 width:30%;display:block;
}

.w_myOnline_proList{
  border-top:1px solid #ebebeb;display:flex;display:-webkit-flex;display: -ms-flexbox;padding:20rpx;
}
.w_myOnline_proList image{
  display:block;width:184rpx;height: 184rpx;margin-right: 20rpx;
}
.w_myOnline_liMids{
  width:40%;flex-grow: 1;-webkit-flex-grow: 1;-webkit-box-flex-group: 1;
}
.w_myOnline_liMids text{
font-size: 28rpx;color:#333;display:block;margin-bottom: 10rpx;text-overflow: ellipsis;overflow:hidden;-webkit-box-orient: vertical;display:-webkit-box;-webkit-line-clamp: 2;
}
.w_myOnline_liMids label{
font-size: 24rpx;color:#E61817;border:1px solid #E61817;padding:1rpx 12rpx;border-radius: 40rpx;
}
.w_myOnline_liRigs{
  width:30%;text-align:right;
}
.w_myOnline_liRigs text,.w_myOnline_liRigs label{
display: block;font-size: 30rpx;
}
.w_myOnline_liRigs text{
color:#333;padding-bottom: 10rpx;
}
.w_myOnline_liRigs label{
color:#999;
}
.w_myOnline_Totals{
  width:100%;text-align: right;font-size: 28rpx;color:#333;padding:0 20rpx 20rpx 20rpx;
  border-bottom:1px solid #ebebeb;margin-top: -20rpx;
}
.w_myOnline_btns{
  padding:20rpx;display:flex;display:-webkit-flex;display: -ms-flexbox;flex-direction:row-reverse;-ms-flex-direction: row-reverse;-webkit-flex-direction: row-reverse;
}
.w_myOnline_btns text{
border:1px solid #ebebeb;border-radius: 60rpx;padding:0 20rpx;
margin-left:20rpx;color:#333;font-size:28rpx;height:60rpx;line-height:56rpx;
}