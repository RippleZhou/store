@import "/style/common.wxss";
.head_tabs {
  display: block;
  width: 100%;
}
.head_tab {
  display: inline-block;
  font-size: 32rpx;
  color: #333;
  line-height: 88rpx;
  padding: 0 40rpx;
}
.active-tab {
  color: #e61817;
}
.home-icon {
  position: absolute;
  top: 0;
  right: 0;
  width: 12%;
  display: block;
  z-index: 101;
}
.home-icon::before {
  font-family: nwscfonts!important;
  font-weight: 400;
  font-style: normal;
  content: "\E008";
  color: #333;
  font-size: 80rpx;
  line-height: 80rpx;
  vertical-align: top;
}
.w_proViewbox {
  margin-top: 88rpx;
}
.swiper-wrap-proimgs {
  width: 100%;
}
.proimgs {
  display: block;
  width: 100%;
}
.product-vedio {
  height: 100%;
  width: 100%;
}
.swiper-wrap {
  position: relative;
}
.swiper-dot {
  position: absolute;
  min-width: 70rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  font-size: 26rpx;
  color: #fff;
  right: 0;
  margin-right: 20rpx;
  left: auto!important;
  bottom: 0;
  margin-bottom: 12rpx;
  z-index: 91;
  background: rgba(0,0,0,.3);
  border-radius: 40rpx;
}
.w_pro_marquees {
  position: absolute;
  left: 0;
  top: 0;
  margin-left: 20rpx;
  margin-top: 10rpx;
  z-index: 92;
  font-size: 24rpx;
  color: #fff;
  height: 40rpx;
  background: rgba(0,0,0,.4);
  border-radius: 40rpx;
  line-height: 40rpx;
  width: 290rpx;
  text-align: center;
}
.pro_marquees {
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  white-space: nowrap;
}
.w_headers  {
  border: none;
}
.w_spike_showTab {
  display: flex;
  width: 100%;
  position: absolute;
  left: 0;
  bottom: 0;
  height: 2.8125rem;
  background: #e61817;
  color: #fff;
  z-index: 96;
}
.w_spike_showLets {
  width: 70%;
  padding: 15rpx 20rpx;
  text-align: left;
}
.spike_price {
  display: block;
  font-size: 36rpx;
  font-weight: 400;
  line-height: 36rpx;
}
.spike_price2 {
  font-size: 24rpx;
  display: block;
  text-decoration: line-through;
}
.w_spike_showRigs {
  width: 30%;
  background: #faccde;
  text-align: center;
}
.spike_label {
    font-size: .8125rem;
    color: #e61817;
    padding-top: 2rpx;
    line-height: 36rpx;
    padding-bottom: 2rpx;
}
.cut-time {
  display: inline-block;
  font-weight: 400;
  color: #fff;
  width: 40rpx;
  height: 40rpx;
  background: #2d2f36;
  border-radius: 6rpx;
  text-align: center;
  line-height: 40rpx;
  margin-right: 4rpx;
  margin-left: 4rpx;
  font-size: 24rpx;
}
.w_proTitles {
  background: #fff;
  padding: 20rpx;
  border-top: 1px solid #ebebeb;
  border-bottom: 1px solid #ebebeb;
}
.pro-title {
  font-size: 32rpx;
  color: #333;
  line-height: 50rpx;
  padding-bottom: 10rpx;
}
.pro-title2 {
  font-size: 24rpx;
  color: #e61817;
  line-height: 60rpx;
}
.pro-desc {
  display: inline-block;
  font-size: 36rpx;
  font-weight: 600;
}

.pro-title-text {
  display: inline-block;
  font-size: 24rpx;
  color: #e61817;
  border: 1px solid #e61817;
  border-radius: 60rpx;
  margin-left: 10rpx;
  padding: 0 10rpx;
  line-height: 32rpx;
  font-weight: 400;
}
.w_txtYels {
  color: #ff9000;
  border: 1px solid #fccf95;
}
.w_txtOranges {
  color: #fdcc07;
  border: 1px solid #fdcc07;
}

.w_proNums {
  background: #fff;
  width: 100%;
  padding: 20rpx;
  margin-bottom: 20rpx;
  text-align: right;
  font-size: 26rpx;
  color: #999;
}
.w_pro_listInfo {
  border-bottom: 1px solid #ebebeb;
  padding: 30rpx 20rpx;
  background: #fff;
  display: flex;
}
.w_pro_listInfo_label {
  font-size: 30rpx;
  color: #999;
  width: 10%;
  text-align: left;
  display: block;
}
.w_pro_listInfo_span {
  position: relative;
  display: block;
  text-align: left;
  font-size: 30rpx;
  width: 90%;
  color: #333;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.w_pro_listInfo_span::before {
  font-family: nwscfonts!important;
  font-weight: 400;
  font-style: normal;
  content: "\E003";
  color: #666;
  font-size: 80rpx;
  vertical-align: middle;
  display: inline-block;
  line-height: 40rpx;
  position: absolute;
  right: 0;
  top: -5rpx;
}
.w_pro_listInfo2 {
  margin-bottom: 20rpx;
}
.w_pro_listInfo_label2 {
  color: #999;
  text-align: left;
  display: block;
  font-size: 24rpx;
  width: 25%;
  position: relative;
  padding-left: 42rpx;
}
.w_pro_listInfo_label2::before {
  font-family: nwscfonts!important;
  font-weight: 400;
  font-style: normal;
  content: "\E018";
  color: #e61817;
  font-size: 80rpx;
  display: inline-block;
  position: absolute;
  left: 0; 
  top: -3rpx;
}
.w_pro_upLoads {
  background: #fff;
  text-align: center;
  font-size: 26rpx;
  color: #999;
  padding: 15rpx 20rpx;
}
.w_pro_ViewCons {
  width: 100%;
  padding-bottom: 100rpx;
}
.product-imgs {
  display: block;
  margin: 0;
  padding: 0;
  width: 100%;
}
.w_proFoots {
  position: fixed;
  z-index: 100;
  left: 0;
  bottom: 0;
  width: 100%;
  background: #fff;
  height: 98rpx;
  border-top: 1px solid #ebebeb;
  display: flex;
}
.foot-span {
  display: block;
  text-align: center;
  height: 98rpx;
  width: 14%;
  font-size: 24rpx;
  color: #333;
  z-index: 100;
  position: relative;
}
.foot-icons::before {
  font-family: nwscfonts!important;
  font-weight: 400;
  font-style: normal;
  color: #333;
  font-size: 80rpx;
  display: block;
  line-height: 49rpx;
}
.w_profot_01::before {
  content: "\E016"
}
.w_profot_02::before {
  content: "\E011"
}
.w_profot_03::before {
  content: "\E017"
}
.w_profot_03 {
  background-color: #fff;
  border: none!important;
  margin: 0;
  padding: 0;
  line-height: 32rpx;
  border-radius: 0;
}
.w_profot_04 {
  width: 29%;
  line-height: 98rpx;
  font-size: 30rpx;
  color: #fff;
  background: #2d2f36;
}
.w_profot_05 {
  width: 29%;
  line-height: 98rpx;
  font-size: 30rpx;
  color: #fff;
  background: linear-gradient(to right,#f90000,#f61e65);
}
.popup-wrap {
  display: block;
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  opacity: 1;
  z-index: 100;
  -webkit-transition: opacity 400ms;
  transition: opacity 400ms;
}
.popup-content {
  background-color: #fff;
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  z-index: 501;
  transition-property: transform;
  transition-duration: 300ms;
  max-height: 100%;
  overflow-y: auto;
  -webkit-overflow-scrolling: touch;
}
.w_popColsed {
  position: absolute;
  right: 0;
  top: 0;
  display: block;
  width: 80rpx;
  height: 80rpx;
  text-align: center;
  z-index: 560;
}
.w_popColsed::before {
  font-family: nwscfonts!important;
  font-weight: 400;
  font-style: normal;
  content: "\E020";
  color: #333;
  font-size: 80rpx;
  vertical-align: middle;
}
.w_pop_proImg {
  width: 100%;
  border-bottom: 1px solid #ebebeb;
  padding: 20rpx;
  padding-bottom: 50rpx;
  display: flex;
}
.popup-product-img {
  width: 218rpx;
  height: 218rpx;
  margin-right: 20rpx;
}
.w_pop_riPric {
  padding-top: 110rpx;
}
.w_pop_riPric_h2 {
  font-size: 30rpx;
  color: #e61817;
}
.w_pop_pric_span {
  display: block;
  font-size: 24rpx;
  color: #333;
}
.w_pop_specs {
  padding: 35rpx 10rpx;
  padding-top: 60rpx;
  border-bottom: 1px solid #ebebeb;
  display: flex;
  flex-wrap: wrap;
}
.sku-item {
  display: block;
  font-size: 28rpx;
  color: #333;
  background: #ebebeb;
  border: 1px solid #ebebeb;
  height: 60rpx;
  padding: 0 30rpx;
  text-align: center;
  border-radius: 10rpx;
  margin-right: 10rpx;
  margin-left: 10rpx;
  line-height: 56rpx;
  margin-bottom: 20rpx;
}
.w_tabSpanCur {
  background: #fef1f6;
  border: 1px solid #e61817;
  color: #e61817;
}
.w_pop_selNums {
  padding: 30rpx 20rpx;
  padding-bottom: 300rpx;
  display: -webkit-box;
  display: flex;
}
.pop_label {
  font-size: 30rpx;
  color: #333;
  flex-grow: 1;
  text-align: left;
}
.w_pop_selBtns {
  width: 220rpx;
  position: relative;
}
.icon-sub {
  width: 60rpx;
}
.icon-add {
  width: 52rpx;
}
.input-num {
  width: 100rpx;
  text-align: center;
  font-size: 20px;
  color: #333;
}
.w_popBtnfot {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  font-size: 30rpx;
  color: #fff;
  text-align: center;
  height: 90rpx;
  line-height: 90rpx;
  display: flex;
}
.w_popBtnfot_span {
  display: block;
  width: 50%;
  text-align: center;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 30rpx;
  color: #fff;
  background: #2d2f36;
}
.w_popBtnReds {
  background: linear-gradient(to right,#f90000,#f61e65);
}
.prevent-icon {
  opacity: 0.2
}



/* 分享弹窗 */
.share-mask {
  position: fixed;
  z-index: 1000;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  background: #000;
  opacity: 0.5;
}
.share-image{position: fixed;width:100%;top:0;}

.dialogNobgs .weui-dialog {
  width: 100%;
  height: 100%;
  background-color: transparent !important;
  max-width: 100%;
}

.dialogNobgs .w_tip_div {
  text-align: center;
  position: relative;
}

.w_tip_fontTxt {
  position: fixed;
  font-size: 30rpx;
  color: #fff;
  left: 35%;
  top: 11%;
  width: 56%;
  line-height:50rpx;
  text-align: left;
  z-index:999999;
}

.dialogNobgs .img {
  width: 100%;
}

.share-image{position: fixed;top: 0;width:100%;z-index:9999999;}
