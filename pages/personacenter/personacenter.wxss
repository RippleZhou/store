    border-radius:180rpx;
    background: #fbe4a3;
    text-align: center;
    line-height: 180rpx;
    left: 50%;
    margin-left: -90rpx;
}
.vux-close {
    position: relative;
    display: inline-block;
    vertical-align: middle;
    color: #999;
    width: 48rpx;
    height: 48rpx;
}
 .vux-close:before {
    content: "";
    position: absolute;
    left: 0;
    top: 22rpx;
    width: 48rpx;
    height: 2rpx;
    background-color: currentColor;
    transform: rotate(-45deg);
}
.vux-close:after {
    content: "";
    position: absolute;
    left: 0;
    top: 22rpx;
    width: 48rpx;
    height: 2rpx;
    background-color: currentColor;
    transform: rotate(45deg);
}
