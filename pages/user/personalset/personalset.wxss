/* pages/personalset/personalset.wxss */
@import "/style/common.wxss";
.w_per_setBox{
  width:100%;
}

.w_per_setLImg{
  background:#fff;padding:30rpx 20rpx;border-bottom:1px solid #f5f5f5;position:relative;
}
.w_per_setLImg image{
  width:90rpx !important;height: 90rpx !important;border-radius:90rpx;margin-right:20rpx;vertical-align:middle;
}
.w_per_setLImg text{
  line-height:90rpx;font-size: 34rpx;color:#333;
}
.w_per_setList{
  background:#fff;border-bottom:1px solid #f5f5f5;padding:30rpx 20rpx;
  display: flex;display: -webkit-flex;position:relative;
}
.w_per_setList::after,.w_per_setLImg::after{
  font-family: nwscfonts!important; font-size:80rpx;
  content:"\E003";color:#666;display:block;position:absolute;right:20rpx;top:0rpx;
}
.w_per_setLImg::after{
  top:30rpx;
}
.w_per_setList label{
  font-size:34rpx;color:#333;width:50%;text-align:left;
}
.w_per_setList text{
  width:50%;text-align:right;color:#999;padding-right:30rpx;
}
.w_per_setMarb{
  margin-bottom:20rpx;
}

.w_per_set_btn{
  width:100%;background:#fff;color:#e61817;font-size:34rpx;padding:30rpx 20rpx;text-align:center;
}