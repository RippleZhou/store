<!--pages/set-paypw/set-paypw.wxml-->
<view class='w_set_paypws'>
  <view class='w_set_payList'>
    <input type='password' placeholder='请输入支付密码' maxlength='6' bindinput='defautValue' id='paypw'></input>
  </view>
  <view class='w_set_payList'>
    <input type='password' placeholder='请确认支付密码' maxlength='6' bindinput='defautValue' id='aginpaypw'></input>
  </view>
  <view class='w_set_payTips'>*支付密码为6位数字</view>
  <view class='w_set_paybtn' bindtap='setpaypw'>确认</view>
</view>
