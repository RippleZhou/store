/* pages/set-paypw/set-paypw.wxss */
page{
  background: #fff;
}
.w_set_paypws{
  padding:30rpx 20rpx;width:100%;
}
.w_set_payList{
  background: #f5f5f5;border:1px solid #ebebeb;height: 88rpx;margin-bottom: 20rpx;border-radius: 10rpx;
  padding:0 10rpx;
}
.w_set_payList input{
  height: 88rpx;padding:0 10rpx;font-size: 30rpx;color: #333;width:100%;
}
.w_set_payTips{
  color:#999;font-size: 26rpx;text-align: left;width:100%;
}

.w_set_paybtn{
  margin: 90rpx;color:#fff;height: 90rpx;line-height: 90rpx;border-radius: 10rpx;
  background: -webkit-linear-gradient(left, #f90000 , #f61e65); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(right, #f90000, #f61e65); /* Opera 11.1 - 12.0 */
  background: -moz-linear-gradient(right, #f90000, #f61e65); /* Firefox 3.6 - 15 */
  background: linear-gradient(to right, #f90000 , #f61e65); /* 标准的语法 */
  text-align: center;font-size: 30rpx;
}