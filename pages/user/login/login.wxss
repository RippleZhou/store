/* pages/my/my.wxss */
@import '../../../style/common.wxss';

page {
  background: #fff;
}
.w_login_logos {
    width: 100%;
    padding-bottom: .9375rem;
    padding-top: 60rpx;
    text-align: center;
}
.logo {
  width: 50%;
}
.w_login_cons {
    padding: 0 47rpx;
}
.w_login_inpLis {
    height: 100rpx;
    margin-top: 36rpx;
    border-bottom: 1px solid #f5f5f5;
}
.input {
    font-size: 32rpx;
    height: 100rpx;
    width: 100%;
    color: #333;
}
.w_login_inpLis2 {
  display: flex;
}
.input2 {
  width: 60%;
}
.w_icon_pwCeye::after {
  font-family: nwscfonts;
  font-weight: 400;
  font-style: normal;
  content: "\E013";
  color: #666;
  font-size: 80rpx;
  vertical-align: middle;
}
.w_icon_pwOeye::after {
    font-family: nwscfonts;
    font-weight: 400;
    font-style: normal;
    content: "\E014";
    color: #666;
    font-size: 80rpx;
    vertical-align: middle;
}
.forget {
      position: relative;
    display: block;
    width: 30%;
    font-size: 28rpx;
    color: #333;
    line-height: 100rpx;
    text-align: right;
}
.w_login_btns {
    padding: 45rpx 0;
    margin-top: 45rpx;
}
.w_login_bTxts {
    width: 100%;
    height: 90rpx;
    line-height: 90rpx;
    border-radius: 90rpx;
    color: #fff;
    text-align: center;
    font-size: 30rpx;
    background: -webkit-gradient(linear,left top, right top,from(#f90000),to(#f61e65));
    background: linear-gradient(to right,#f90000,#f61e65);
}
.w_login_texts {
  display: flex;
}
.w_login_texts-link {
    font-size: 30rpx;
    color: #333;
    display: block;
    width: 50%;
}
