    color: #fff;
    border-radius: 30px;
    height: 2.5rem;
    line-height: 2.5rem;
    text-align: center;
}

 .popup-price {
    color: #e53549;
    position: absolute;
    top: 2.25rem;
    width: 100%;
    text-align: center;
    font-size: 3.0625rem;
}

 .tips2 {
    position: absolute;
    bottom: 1.25rem;
    width: 100%;
    text-align: center;
    font-size: .75rem;
    color: #fff;
}

