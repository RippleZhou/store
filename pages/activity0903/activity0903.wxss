.mask{
      position: fixed;
    z-index: 1000;
    top: 0;
    right: 0;
    left: 0;
    bottom: 0;
    opacity: .5;
    background: #000;
}
.weui-dialog {
    position: fixed;
    /* display: table; */
    z-index: 5000;
    width: 80%;
    max-width: 300px;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    margin: auto;
    background-color: #fff;
    text-align: center;
    border-radius: 3px;
    overflow: hidden;
}

.pink-background {
    background-color: #fcf2de;
    position: relative;
    padding-top: 2.0625rem;
    padding-bottom: 1px;

}

 .activity0903-popup-cancel {
    position: absolute;
    top: -6px;
    left: 12px;
    z-index: 10;
}

.activity0903-popup-cancel:before {
    font-family: nwscfonts!important;
    font-weight: 400;
    font-style: normal;
    content: "\E020";
    color: #999;
    font-size: 2.5rem;
    vertical-align: middle;
}

.moneystyle{
    font-size: .9375rem;
}

.pink-background .h2 {
    font-size: 1.5rem;
    color: #e53549;
    text-align: center;
