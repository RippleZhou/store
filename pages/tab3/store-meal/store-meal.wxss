@import "/style/common.wxss";
page {
  background-color: #fff;
}
.s_container {
  margin-bottom: 95rpx;
  background-color: #f5f5f5;
} 
.cashCoupon {
  width: 100%;
  background: #fff;
  padding: 20rpx;
}
.w_cashCoupon {
  padding: 0 0;
  border-radius: 15rpx;
  margin-top: 20rpx;
  position: relative;
  padding-top: 10rpx;
}

.activity {
  padding: 0 50rpx;
}
.coupon-h2 {
  font-size: 34rpx;
  margin-bottom: 20rpx;
}
.w_redTips {
  display: inline-block;
  color: #e61817;
  border: 1px solid #e61817;
  font-size: 24rpx;
  border-radius: 36rpx;
  line-height: 30rpx;
  padding: 0 10rpx;
}
.cashCouponDetail_P {
  font-size: 24rpx;
  color: #aaa;
  margin-top: 10rpx;
}
.nowprice {
  font-size: 32rpx;
  color: #e61817;
}
.line-through {
  text-decoration: line-through;
}
.w_redTips2 {
  color: #e61817;
  font-size: 24rpx;
  margin-top: 0;
  position: relative;
}
.activLefs {
  display: block;
  flex-grow: 1;
  text-align: left;
  font-size: 24rpx;
  color: #aaa;
  line-height: 50rpx;
}
.w_rigde_rigIcons {
  flex-grow: 1;
  text-align: right;
  padding-top: 0;
}
.watchedicon:before {
  position: relative;
  margin-bottom: 10rpx;
  display: inline-block;
  margin-right: 10rpx;
  font-size: 60rpx;
  font-family: nwscfonts!important;
  font-weight: 400;
  font-style: normal;
  content: "\E075";
  color: #aaa;
  vertical-align: middle;
}
.watchedicon {
  color: #999;
  font-size: 26rpx;
  display: inline-block;
  margin-left: 30rpx;
}
.cashCouponExplain {
  margin-top: 20rpx;
  padding: 20rpx;
  background: #fff;
  font-size: 28rpx;
  line-height:61rpx;
}
.detail-subtitle {
  color: #000;
  font-weight: bolder;
  display: inline-block;
  margin-right: 20rpx;
}
.content {
  color: #aaa;
}
.content1 {
  display: inline-block;
  margin-left: 30rpx;
}
.storeIntro {
  padding: 20rpx 10rpx;
  padding-right: 20rpx;
  background: #fff;
  margin-top: 20rpx;
}
.store-img {
  width: 150rpx;
  height: 150rpx;
  object-fit: cover;
}
.storedetailright {
  -webkit-box-flex: 1;
  -ms-flex-positive: 1;
  flex-grow: 1;
  font-size: 24rpx;
  color: #aaa;
  margin-left: 20rpx;
}
.store-right-b {
  font-size: 28rpx;
  color: #333;
}
.store-float-right {
  float: right;
}
.store-type {
  margin-top: 20rpx;
}
.w_storeDe_list {
  width: 100%;
  display: -webkit-box;
  display: flex;
  display: -ms-flexbox;
  padding-top: 30rpx;
}
.w_storeDe_list_span {
  display: block;
  width: 100%;
  font-size: 26rpx;
  color: #333;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.w_storeDe_list2 {
  padding-top: 0;
  padding-bottom: 32rpx;
}
.list_span2 {
  width: 85%;
  padding-right: 10rpx;
}
.call-phone {
  border-left: 1px solid #ebebeb;
  display: block;
  text-align: center;
  width: 15%;
  position: relative;
  top: -20rpx;
}
.call-phone::before {
  display: block;
  font-size: 120rpx;
  line-height: 70rpx;
  font-family: nwscfonts!important;
  font-weight: 400;
  font-style: normal;
  color: #f80d2a;
  content: "\E047";
}
.storeBottom {
  position: fixed;
  bottom: 0;
  height: 95rpx;
  width: 100%;
  text-align: center;
  line-height: 95rpx;
  border-top: 1px solid #ebebeb;
}
.rushbuy {
  float: left;
  width: 50%;
  color: #e61817;
  background: #fff;
}
.cutprice {
  float: left;
  width: 50%;
  background: #e61817;
  color: #fff;
}
.gooncutprice {
  float: left;
  width: 50%;
  background: #ff9000;
  color: #fff;
}
.rish-all {
  background-color: #ccc;
  color: #fff;
}
.swiper-wrapper {
  height: 256rpx;
}
.banner-img {
  display: block;
  width: 100%;
  border-radius: 10rpx;
}
.activ-tips {
  float: right;
  font-size: 24rpx;
  color: #aaa;
}
.mealdetail {
  margin-top: 20rpx;
  background: #fff;
  padding: 25rpx 20rpx;
}
.mealdetail-h2 {
  font-size: 28rpx;
  color: #333;
  font-weight: 600;
}
.detail-content {
  padding: 25rpx 0;
}
.detail-content-p {
  font-size: 28rpx;
  line-height: 28rpxx;
  margin-bottom: 15rpx;
}
.detail-content-number {
  color: #aaa;
}
.detail-content-price {
  float: right;
  font-size: 26rpx;
}
.reminder {
  font-size: 28rpx;
  color: #333;
  font-weight: 600;
}
.reminber-content {
  font-size: 26rpx;
  margin-top: 5rpx;
}